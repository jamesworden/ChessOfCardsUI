{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "LanesFrontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "projects/client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/LanesFrontend",
            "index": "projects/client/src/index.html",
            "main": "projects/client/src/main.ts",
            "polyfills": "projects/client/src/polyfills.ts",
            "tsConfig": "projects/client/tsconfig.app.json",
            "assets": [
              "projects/client/src/favicon.ico",
              "projects/client/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/client/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/client/src/environments/environment.ts",
                  "with": "projects/client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "LanesFrontend:build:production"
            },
            "development": {
              "browserTarget": "LanesFrontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "LanesFrontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/client/src/test.ts",
            "polyfills": "projects/client/src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "projects/client/src/favicon.ico",
              "projects/client/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/client/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "LanesFrontendTestClient": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/test-client",
      "sourceRoot": "projects/test-client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-client",
            "index": "projects/test-client/src/index.html",
            "main": "projects/test-client/src/main.ts",
            "polyfills": "projects/test-client/src/polyfills.ts",
            "tsConfig": "projects/test-client/tsconfig.app.json",
            "assets": [
              "projects/test-client/src/favicon.ico",
              "projects/test-client/src/assets"
            ],
            "styles": ["projects/test-client/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/test-client/src/environments/environment.ts",
                  "with": "projects/test-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test-client:build:production"
            },
            "development": {
              "browserTarget": "test-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test-client/src/test.ts",
            "polyfills": "projects/test-client/src/polyfills.ts",
            "tsConfig": "projects/test-client/tsconfig.spec.json",
            "karmaConfig": "projects/test-client/karma.conf.js",
            "assets": [
              "projects/test-client/src/favicon.ico",
              "projects/test-client/src/assets"
            ],
            "styles": ["projects/test-client/src/styles.css"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "LanesFrontend"
}
